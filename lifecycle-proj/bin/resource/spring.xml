<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans"
	xmlns:context="http://www.springframework.org/schema/context"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xsi:schemaLocation=" http://www.springframework.org/schema/beans 
	http://www.springframework.org/schema/beans/spring-beans.xsd
	http://www.springframework.org/schema/context http://www.springframework.org/schema/context/spring-context.xsd">


	<bean class="com.xworkz.spring.component" id="camera">
	<constructor-arg value="Sony"></constructor-arg>
	<constructor-arg value="54.06"></constructor-arg>
	</bean>
	
	<bean class="com.xworkz.spring.component" id="stadium">
	<constructor-arg value="chinnaswamy"></constructor-arg>
	<constructor-arg value="Bangalore"></constructor-arg>
	</bean>
	
	<bean class="java.lang.String" id="string">
	<constructor-arg value="xworkz"></constructor-arg>
	</bean>
	
	<bean class="java.lang.Integer" id="integer">
	<constructor-arg value="2000"></constructor-arg>
	</bean>
	
</beans>